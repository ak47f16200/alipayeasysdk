{
	"name":"luwc/alipayeasysdk",
	"description":"基于支付宝官方 Alipay Easy SDK,增加openid支持",
	"type":"library",
	"version":"1.0.3",
	"authors":[
		{
			"name":"luwc",
			"email":"ak47f16200@126.com"
		}
	],
	"autoload":{
		"psr-4":{
			"Alipay\\EasySDK\\":"php/src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Alipay\\EasySDK\\Test\\": "php/test/"
		}
	},
	"license":"Apache-2.0",
	"minimum-stability":"dev",
	"require":{
		"php": ">=7.0",
		"ext-curl":"*",
		"ext-ctype":"*",
		"ext-dom":"*",
		"ext-fileinfo": "*",
		"ext-json":"*",
		"ext-libxml":"*",
		"ext-simplexml":"*",
		"ext-mbstring":"*",
		"ext-openssl":"*",
		"ext-xmlwriter": "*",
		"guzzlehttp/guzzle":">=6.3",
		"alibabacloud/tea": "^3.1",
		"alibabacloud/tea-fileform": "^0.3.2"
    },
	"require-dev": {
		"phpunit/phpunit": "^7.5"
	},
	"repositories":{
		"packagist":{
			"type":"composer",
			"url":"https://mirrors.aliyun.com/composer/"
		}
	}
}
